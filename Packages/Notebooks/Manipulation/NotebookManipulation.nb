Cell[BoxData[
 RowBox[{
  RowBox[{"nbPut2", "[", 
   RowBox[{"enb_NotebookObject", ",", " ", "nb_Notebook"}], "]"}], ":=", "\n",
   "\t", 
  RowBox[{"With", "[", "\n", "\t  ", 
   RowBox[{
    RowBox[{"{", "\n", "     ", 
     RowBox[{
      RowBox[{"mcells", "=", "\n", "      ", 
       RowBox[{"Sequence", "@@", 
        RowBox[{"Flatten", "@", "\n", "       ", 
         RowBox[{"Map", "[", "\n", "        ", 
          RowBox[{
           RowBox[{
            RowBox[{"{", "\n", "         ", 
             RowBox[{
              RowBox[{"FrontEnd`SetOptions", "[", 
               RowBox[{"#", ",", " ", 
                RowBox[{"Deletable", "->", "True"}]}], "]"}], ",", "\n", 
              "         ", 
              RowBox[{"FrontEnd`NotebookDelete", "[", "#", "]"}]}], "\n", 
             "         ", "}"}], "&"}], ",", "\n", "        ", 
           RowBox[{"Cells", "[", "enb", "]"}]}], "\n", "        ", 
          "]"}]}]}]}], ",", "\n", "     ", 
      RowBox[{"ops", "=", 
       RowBox[{"Options", "[", "enb", "]"}]}]}], "\n", "    ", "}"}], ",", 
    "\n", "    ", 
    RowBox[{"MathLink`CallFrontEnd", "@", 
     RowBox[{"{", "\n", "     ", 
      RowBox[{
       RowBox[{"FrontEnd`SetOptions", "[", 
        RowBox[{"enb", ",", " ", "ops"}], "]"}], ",", "\n", "     ", "mcells",
        ",", "\n", "     ", 
       RowBox[{"FrontEnd`SelectionMove", "[", 
        RowBox[{"enb", ",", " ", "Before", ",", " ", "Notebook"}], "]"}], ",",
        "\n", "     ", 
       RowBox[{"FrontEnd`NotebookWrite", "[", 
        RowBox[{"enb", ",", " ", 
         RowBox[{"nb", "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", " ", "None", ",", "\n", 
         "      ", 
         RowBox[{"AutoScroll", "->", "False"}]}], "\n", "      ", "]"}]}], 
      "\n", "     ", "}"}]}]}], "\n", "   ", 
   "]"}]}]], \
"CodeInput",ExpressionUUID->"e5fdcd7d-7c55-4d9e-8420-705095f6bd7a"],

