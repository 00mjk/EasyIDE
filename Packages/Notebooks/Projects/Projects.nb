Cell["\[LeftCeiling]SaveProjectFileAs\[RightFloor]", "CodeSubsubsection",
 Evaluatable->True],

Cell[BoxData[{
 RowBox[{"SaveProjectFileAs", "//", "Clear"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SaveProjectFileAs", "[", 
    RowBox[{"nb_", ",", " ", 
     RowBox[{"file1_String", "?", 
      RowBox[{"(", "FileExistsQ", ")"}]}], ",", " ", 
     RowBox[{"fileName_String", "?", 
      RowBox[{"(", 
       RowBox[{"Not", "@*", "DirectoryQ"}], ")"}]}]}], "]"}], ":=", "\n", 
   "\t", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"tabs", " ", "=", " ", 
        RowBox[{"IDEData", "[", 
         RowBox[{"nb", ",", " ", "\"\<Tabs\>\""}], "]"}]}], ",", " ", 
       "newTabName", ",", " ", "fileHavingTab"}], "}"}], ",", "\n", "\t  ", 
     RowBox[{"Replace", "[", "\n", "\t    ", 
      RowBox[{
       RowBox[{"RenameFile", "[", 
        RowBox[{"file1", ",", " ", "fileName"}], "]"}], ",", "\n", "\t    ", 
       RowBox[{"s_String", "\[RuleDelayed]", 
        RowBox[{"(", "\n", "\t      ", 
         RowBox[{
          RowBox[{"fileHavingTab", " ", "=", " ", "\n", "\t        ", 
           RowBox[{"SelectFirst", "[", 
            RowBox[{"tabs", ",", " ", "\n", "\t          ", 
             RowBox[{
              RowBox[{"Quiet", "[", "\n", "\t            ", 
               RowBox[{
                RowBox[{"ExpandFileName", "@", 
                 RowBox[{"Lookup", "[", 
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", " ", 
                   "\"\<File\>\""}], "]"}]}], "\[Equal]", "\n", 
                "\t              ", 
                RowBox[{"ExpandFileName", "@", "file1"}]}], "\n", 
               "\t            ", "]"}], "&"}]}], "\n", "\t          ", 
            "]"}]}], ";", "\n", "\t      ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"!", 
             RowBox[{"MissingQ", "[", "fileHavingTab", "]"}]}], ",", "\n", 
            "\t        ", 
            RowBox[{
             RowBox[{"newTabName", " ", "=", " ", 
              RowBox[{"GetFileTabName", "[", 
               RowBox[{"nb", ",", " ", "fileName"}], "]"}]}], ";", "\n", 
             "\t        ", 
             RowBox[{
              RowBox[{"IDEData", "[", 
               RowBox[{"nb", ",", " ", "\"\<Tabs\>\""}], "]"}], " ", "=", " ",
               "\n", "  \t        ", 
              RowBox[{"Replace", "[", 
               RowBox[{"tabs", ",", " ", "\n", "  \t          ", 
                RowBox[{"fileHavingTab", "\[RuleDelayed]", 
                 RowBox[{"(", "\n", "  \t            ", 
                  RowBox[{
                  "newTabName", "\[RuleDelayed]", "\n", "  \t              ", 
                   
                   RowBox[{"Replace", "[", 
                    RowBox[{
                    RowBox[{"fileHavingTab", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", " ", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"\"\<File\>\"", "\[Rule]", "_"}], ")"}], 
                    "\[RuleDelayed]", 
                    RowBox[{"(", 
                    RowBox[{"\"\<File\>\"", "\[Rule]", "fileName"}], ")"}]}], 
                    ",", " ", "1"}], "]"}]}], "\n", "  \t            ", 
                  ")"}]}], ",", "\n", "  \t          ", "1"}], "\n", 
               "  \t          ", "]"}]}], ";", "\n", "  \t      ", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"IDEData", "[", 
                 RowBox[{"nb", ",", " ", "\"\<ActiveTab\>\""}], "]"}], " ", 
                "\[Equal]", " ", 
                RowBox[{"fileHavingTab", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], ",", "\n", "  \t        ", 
               
               RowBox[{
                RowBox[{"IDEData", "[", 
                 RowBox[{"nb", ",", " ", "\"\<ActiveTab\>\""}], "]"}], " ", 
                "=", " ", "newTabName"}]}], "\n", "  \t        ", "]"}]}]}], 
           "\n", "\t        ", "]"}]}], "\n", "\t      ", ")"}]}]}], "\n", 
      "\t    ", "]"}]}], "\n", "\t  ", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SaveProjectFileAs", "[", 
    RowBox[{"nb_", ",", " ", 
     RowBox[{"file_String", "?", 
      RowBox[{"(", "FileExistsQ", ")"}]}]}], "]"}], ":=", "\n", "  ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"f", " ", "=", " ", 
       RowBox[{"SystemDialogInput", "[", 
        RowBox[{"\"\<FileSave\>\"", ",", " ", 
         RowBox[{"IDEPath", "[", "nb", "]"}], ",", " ", 
         RowBox[{"WindowTitle", "\[Rule]", "\"\<New name\>\""}]}], "]"}]}], 
      "}"}], ",", "\n", "    ", 
     RowBox[{"SaveProjectFileAs", "[", 
      RowBox[{"nb", ",", " ", "file", ",", " ", "f"}], "]"}]}], "\n", "    ", 
    "]"}]}], ";"}]}], "CodeInput"]
}, Open  ]],

