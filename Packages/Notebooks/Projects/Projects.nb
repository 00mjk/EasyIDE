Cell[BoxData[{
 RowBox[{"NewProjectFile", "//", "Clear"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"NewProjectFile", "[", 
    RowBox[{"nb_", ",", " ", 
     RowBox[{"fileName_String", "?", 
      RowBox[{"(", 
       RowBox[{"Not", "@*", "DirectoryQ"}], ")"}]}]}], "]"}], ":=", "\n", 
   "\t", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"fn", "=", 
       RowBox[{"IDEPath", "[", 
        RowBox[{"nb", ",", " ", "fileName"}], "]"}]}], "}"}], ",", "\n", 
     "\t\t", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"!", 
         RowBox[{"FileExistsQ", "[", "fn", "]"}]}], ",", " ", 
        RowBox[{"CreateFile", "[", "fn", "]"}]}], "]"}], ";", "\n", "\t\t", 
      RowBox[{"IDEOpen", "[", 
       RowBox[{"nb", ",", " ", "fn"}], "]"}], ";"}]}], "\n", "\t\t", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"NewProjectFile", "[", 
    RowBox[{"nb_", ",", " ", 
     RowBox[{"dir_String", "?", "DirectoryQ"}]}], "]"}], ":=", "\n", "  ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"f", " ", "=", " ", 
       RowBox[{"SystemDialogInput", "[", 
        RowBox[{"\"\<FileSave\>\"", ",", " ", "dir", ",", " ", 
         RowBox[{"WindowTitle", "\[Rule]", "\"\<New file\>\""}]}], "]"}]}], 
      "}"}], ",", "\n", "    ", 
     RowBox[{"NewProjectFile", "[", 
      RowBox[{"nb", ",", " ", "f"}], "]"}]}], "\n", "    ", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"NewProjectFile", "[", "nb_", "]"}], ":=", "\n", "  ", 
  RowBox[{"NewProjectFile", "[", 
   RowBox[{"nb", ",", " ", 
    RowBox[{"IDEPath", "[", "nb", "]"}]}], "]"}]}]}], "CodeInput"]
}, Closed]],

