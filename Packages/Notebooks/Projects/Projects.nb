      RowBox[{
       RowBox[{"CurrentValue", "[", 
        RowBox[{"nb", ",", " ", "WindowTitle"}], "]"}], " ", "=", " ", 
       RowBox[{"getIDETitleBar", "[", "dir", "]"}]}], ";"}]}], "\n", "\t\t", 
